    <div class="container">
        <div class="content">
            <h1>비밀번호 찾기</h1>
            <form action="${contextPath}/login/changePw" method="post" onsubmit="return validate();">
                <div class="email">
                    <input type="email" id="email" name="memberEmail" required>
                    <label for="email">이메일</label>
                </div>
                <div class="certification">
                    <input type="text" id="code" required>
                    <label for="code">인증번호</label>
                </div>
                <button type="button" class="send">전송</button>
                <button type="submit" class="code">입력</button>
            </form>
            <a href="${contextPath}/login/emailView">이메일 찾기</a><br>
            <a href="${contextPath}/login/loginView">로그인</a>
        </div>
    </div>
    <script src="//cdn.jsdelivr.net/npm/sweetalert2@10"></script>
    <script>
   		var signUpCheck = {
			"check" : false,
		};
    
    	var code = 0;
        $(".send").on('click' ,()=> {
            
            $(".email").css('display', 'none');
            $(".certification").css('display', 'block');

            $(".send").css('display', 'none');
            $(".code").css('display', 'block');
            
	        $.ajax({
	        	url: "${contextPath}/login/findPw",
	        	data:{
	        		"memberEmail": $("#email").val()
	        	},
	        	type: "post",
	        	success: function(result){
	        		console.log(result)
	        		code = result;
	        		console.log("성공")
	        	}, 
	        	error: function(){
	        		console.log("실패")
	        	}
	        	
	        })
        })
        
        
        
