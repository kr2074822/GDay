<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL" />
		<!-- DB로 전달되는 값 중 null이 있으면 에러가 아닌 NULL 자체로 인식  -->
	</settings>
	
	<typeAliases>
		<typeAlias type="team.project.gday.member.model.vo.Member" alias="Member"/>
		<typeAlias type="team.project.gday.calendar.model.vo.Calendar" alias="Calendar"/>
	</typeAliases>

	<mappers>
		<mapper resource="/mappers/member-mapper.xml"/>
		<mapper resource="/mappers/calendar-mapper.xml"/>
	</mappers>
</configuration>